@startuml
package DiamTelecom {
    class Subscriber {
        +id
        +msisdn
        +imsi
    }

    class Subscribers {
        +subscribers: List[Subscriber]
    }

    class Carrier {
        +id
        +name
        +subscriber: Subscribers
    }

    Carrier *-- Subscribers
    Subscribers o-- Subscriber
}
@enduml
